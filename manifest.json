{ 
    "manifest_version": 3, 
    "name": "NASA Astronomy Wallpaper", 
    "version": "1.0", 
    "description": "Sets NASA's Astronomy Picture of the Day as your new tab background.", 
    "permissions": [
        "storage",
        "alarms"
    ], 
    "icons": { 
        "16": "icon.png", 
        "48": "icon.png", 
        "128": "icon.png" 
    }, 
    "chrome_url_overrides": { 
        "newtab": "newtab.html" 
    }, 
    "background": { 
        "service_worker": "background.js" 
    }, 
    "host_permissions": [ 
        "https://api.nasa.gov/*"
    ] ,
    "action":{
        "default_popup": "file.html"
    }
}